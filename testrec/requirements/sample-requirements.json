{
  "version": "1.0",
  "programs": [
    {
      "programId": "aws_preseed_innovative_solutions",
      "programName": "aws Preseed – Innovative Solutions",
      "programType": "grant",
      "targetPersonas": ["solo", "sme"],
      "eligibility": [
        {
          "id": "elig_1",
          "category": "eligibility",
          "type": "boolean",
          "title": "Natural person or company ≤6 months after registration",
          "description": "Applicant must be a natural person (team) or a company registered no more than 6 months ago",
          "isRequired": true,
          "priority": "critical",
          "validationRules": [
            {
              "type": "required",
              "message": "This is a critical eligibility requirement"
            }
          ],
          "examples": ["GmbH registered 3 months ago", "Einzelunternehmen", "Team of individuals"],
          "guidance": "Check company registration date or team formation date"
        },
        {
          "id": "elig_2",
          "category": "eligibility",
          "type": "boolean",
          "title": "Project location in Austria",
          "description": "The project must be executed in Austria",
          "isRequired": true,
          "priority": "critical",
          "validationRules": [
            {
              "type": "required",
              "message": "Austrian location is mandatory"
            }
          ],
          "examples": ["Vienna", "Salzburg", "Graz"],
          "guidance": "Provide Austrian address and confirm project execution location"
        },
        {
          "id": "elig_3",
          "category": "eligibility",
          "type": "boolean",
          "title": "High degree of innovation",
          "description": "Project must demonstrate high innovation with scalable market potential",
          "isRequired": true,
          "priority": "high",
          "validationRules": [
            {
              "type": "required",
              "message": "Innovation is a key requirement"
            }
          ],
          "examples": ["Novel technology", "Unique business model", "Disruptive approach"],
          "guidance": "Clearly articulate what makes your solution innovative"
        }
      ],
      "documents": [
        {
          "id": "doc_1",
          "category": "documentation",
          "type": "document",
          "title": "Business Plan",
          "description": "Comprehensive business plan following program guidelines",
          "isRequired": true,
          "priority": "critical",
          "validationRules": [
            {
              "type": "required",
              "message": "Business plan is mandatory"
            }
          ],
          "examples": ["15-25 pages", "Executive summary", "Financial projections"],
          "guidance": "Use program-specific template and guidelines"
        },
        {
          "id": "doc_2",
          "category": "documentation",
          "type": "document",
          "title": "Project Description",
          "description": "Detailed project description with technical approach",
          "isRequired": true,
          "priority": "high",
          "validationRules": [
            {
              "type": "required",
              "message": "Project description is required"
            }
          ],
          "examples": ["Technical specifications", "Implementation plan", "Timeline"],
          "guidance": "Include technical details and implementation approach"
        },
        {
          "id": "doc_3",
          "category": "documentation",
          "type": "document",
          "title": "Team CVs",
          "description": "Curriculum vitae of all team members",
          "isRequired": true,
          "priority": "high",
          "validationRules": [
            {
              "type": "required",
              "message": "Team CVs are required"
            }
          ],
          "examples": ["Academic background", "Professional experience", "Relevant skills"],
          "guidance": "Highlight relevant experience and qualifications"
        }
      ],
      "financial": [
        {
          "id": "fin_1",
          "category": "financial",
          "type": "numeric",
          "title": "Funding Amount",
          "description": "Requested funding amount (max €100,000)",
          "isRequired": true,
          "priority": "critical",
          "validationRules": [
            {
              "type": "max",
              "value": 100000,
              "message": "Maximum funding amount is €100,000"
            },
            {
              "type": "min",
              "value": 10000,
              "message": "Minimum funding amount is €10,000"
            }
          ],
          "examples": ["€50,000", "€75,000", "€100,000"],
          "guidance": "Justify the requested amount with detailed budget breakdown"
        },
        {
          "id": "fin_2",
          "category": "financial",
          "type": "document",
          "title": "Budget Breakdown",
          "description": "Detailed budget breakdown showing use of funds",
          "isRequired": true,
          "priority": "critical",
          "validationRules": [
            {
              "type": "required",
              "message": "Budget breakdown is mandatory"
            }
          ],
          "examples": ["Personnel costs", "Equipment", "Marketing", "Operations"],
          "guidance": "Provide detailed breakdown by category with justification"
        },
        {
          "id": "fin_3",
          "category": "financial",
          "type": "numeric",
          "title": "Co-financing",
          "description": "Required co-financing (20% of total project costs)",
          "isRequired": true,
          "priority": "high",
          "validationRules": [
            {
              "type": "min",
              "value": 0.2,
              "message": "Minimum 20% co-financing required"
            }
          ],
          "examples": ["€10,000 own funds", "€15,000 from other sources"],
          "guidance": "Demonstrate ability to provide co-financing"
        }
      ],
      "technical": [
        {
          "id": "tech_1",
          "category": "technical",
          "type": "selection",
          "title": "Technology Readiness Level",
          "description": "Current TRL of the technology (TRL 3-6 preferred)",
          "isRequired": true,
          "priority": "high",
          "validationRules": [
            {
              "type": "required",
              "message": "TRL assessment is required"
            }
          ],
          "examples": ["TRL 3: Proof of concept", "TRL 4: Lab validation", "TRL 5: Prototype"],
          "guidance": "Assess current technology maturity level"
        },
        {
          "id": "tech_2",
          "category": "technical",
          "type": "boolean",
          "title": "Innovation Focus",
          "description": "Project focuses on innovation areas (diversity, environment, health, etc.)",
          "isRequired": true,
          "priority": "high",
          "validationRules": [
            {
              "type": "required",
              "message": "Innovation focus is required"
            }
          ],
          "examples": ["Environmental solutions", "Health technology", "Education innovation"],
          "guidance": "Align with program focus areas"
        }
      ],
      "scoringWeights": {
        "eligibility": 0.3,
        "documents": 0.2,
        "financial": 0.2,
        "technical": 0.15,
        "legal": 0.05,
        "timeline": 0.05,
        "geographic": 0.05,
        "team": 0.0,
        "project": 0.0,
        "compliance": 0.0
      },
      "decisionTreeQuestions": [
        {
          "id": "q_company_stage",
          "requirementId": "elig_1",
          "question": "What is your company stage?",
          "type": "single",
          "options": [
            { "value": "PRE_COMPANY", "label": "Just an idea or team forming" },
            { "value": "INC_LT_6M", "label": "Recently started (less than 6 months)" },
            { "value": "INC_6_36M", "label": "Early stage (6 months to 3 years)" }
          ],
          "validation": [
            {
              "type": "required",
              "message": "Company stage is required"
            }
          ],
          "followUpQuestions": [],
          "skipConditions": []
        },
        {
          "id": "q_austria_location",
          "requirementId": "elig_2",
          "question": "Will your project be executed in Austria?",
          "type": "single",
          "options": [
            { "value": "YES", "label": "Yes" },
            { "value": "NO", "label": "No" }
          ],
          "validation": [
            {
              "type": "required",
              "message": "Austrian location is required"
            }
          ],
          "followUpQuestions": [],
          "skipConditions": [
            {
              "condition": "answers.q_austria_location === 'NO'",
              "skipTo": "ineligible"
            }
          ]
        },
        {
          "id": "q_funding_amount",
          "requirementId": "fin_1",
          "question": "How much funding are you seeking?",
          "type": "number",
          "options": [],
          "validation": [
            {
              "type": "required",
              "message": "Funding amount is required"
            },
            {
              "type": "min",
              "value": 10000,
              "message": "Minimum amount is €10,000"
            },
            {
              "type": "max",
              "value": 100000,
              "message": "Maximum amount is €100,000"
            }
          ],
          "followUpQuestions": [],
          "skipConditions": []
        }
      ],
      "editorSections": [
        {
          "id": "executive_summary",
          "title": "Executive Summary",
          "required": true,
          "template": "Our innovative project [PROJECT_NAME] seeks [FUNDING_AMOUNT] in funding to [PROJECT_GOAL]. The project addresses [PROBLEM_STATEMENT] through [SOLUTION_APPROACH] and will be executed in Austria over [TIMELINE] months.",
          "guidance": "Keep concise but compelling. Highlight innovation and impact.",
          "requirements": ["elig_1", "elig_2", "fin_1"],
          "prefillData": {
            "PROJECT_NAME": "answers.business_name",
            "FUNDING_AMOUNT": "answers.funding_amount",
            "PROJECT_GOAL": "answers.project_goal",
            "PROBLEM_STATEMENT": "answers.problem_statement",
            "SOLUTION_APPROACH": "answers.solution_approach",
            "TIMELINE": "answers.timeline"
          }
        },
        {
          "id": "project_description",
          "title": "Project Description",
          "required": true,
          "template": "## Project Overview\n\n[PROJECT_DESCRIPTION]\n\n## Technical Approach\n\n[TECHNICAL_APPROACH]\n\n## Innovation Aspects\n\n[INNOVATION_ASPECTS]\n\n## Implementation Timeline\n\n[TIMELINE_DETAILS]",
          "guidance": "Focus on innovation, feasibility, and alignment with program goals.",
          "requirements": ["tech_1", "tech_2", "doc_2"],
          "prefillData": {
            "PROJECT_DESCRIPTION": "answers.project_description",
            "TECHNICAL_APPROACH": "answers.technical_approach",
            "INNOVATION_ASPECTS": "answers.innovation_aspects",
            "TIMELINE_DETAILS": "answers.timeline_details"
          }
        },
        {
          "id": "financial_plan",
          "title": "Financial Planning",
          "required": true,
          "template": "## Funding Request\n\n**Amount**: €[FUNDING_AMOUNT]\n**Duration**: [PROJECT_DURATION] months\n**Co-financing**: €[CO_FINANCING] ([CO_FINANCING_PERCENTAGE]%)\n\n## Budget Breakdown\n\n[BUDGET_BREAKDOWN]\n\n## Financial Projections\n\n[FINANCIAL_PROJECTIONS]",
          "guidance": "Include detailed budget breakdown and funding requirements.",
          "requirements": ["fin_1", "fin_2", "fin_3"],
          "prefillData": {
            "FUNDING_AMOUNT": "answers.funding_amount",
            "PROJECT_DURATION": "answers.project_duration",
            "CO_FINANCING": "answers.co_financing",
            "CO_FINANCING_PERCENTAGE": "answers.co_financing_percentage",
            "BUDGET_BREAKDOWN": "answers.budget_breakdown",
            "FINANCIAL_PROJECTIONS": "answers.financial_projections"
          }
        }
      ],
      "readinessCriteria": [
        {
          "id": "criterion_1",
          "requirementId": "elig_1",
          "title": "Company Stage Eligibility",
          "description": "Verify company is within 6 months of registration or team formation",
          "checkType": "validation",
          "validator": "function(data) { return { passed: data.company_stage === 'INC_LT_6M', message: 'Company must be within 6 months of registration', score: data.company_stage === 'INC_LT_6M' ? 100 : 0 }; }",
          "weight": 1.0
        },
        {
          "id": "criterion_2",
          "requirementId": "elig_2",
          "title": "Austrian Location",
          "description": "Confirm project will be executed in Austria",
          "checkType": "validation",
          "validator": "function(data) { return { passed: data.austria_location === 'YES', message: 'Project must be executed in Austria', score: data.austria_location === 'YES' ? 100 : 0 }; }",
          "weight": 1.0
        },
        {
          "id": "criterion_3",
          "requirementId": "fin_1",
          "title": "Funding Amount",
          "description": "Verify funding amount is within limits (€10,000 - €100,000)",
          "checkType": "validation",
          "validator": "function(data) { const amount = parseInt(data.funding_amount); return { passed: amount >= 10000 && amount <= 100000, message: 'Funding amount must be between €10,000 and €100,000', score: amount >= 10000 && amount <= 100000 ? 100 : 0 }; }",
          "weight": 1.0
        }
      ],
